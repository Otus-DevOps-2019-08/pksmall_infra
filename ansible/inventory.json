{
    "_meta": {
        "hostvars": {
            "appserver": {
                "ansible_host": "APPIP"
            },
            "dbserver": {
                "ansible_host": "DBIP"
            }
        }
    },
    "all": {
        "children": [
            "app",
            "db"
        ]
    },
    "app": {
        "hosts": [
            "appserver"
        ]
    },
    "db": {
        "hosts": [
            "dbserver"
        ]
    }
}
